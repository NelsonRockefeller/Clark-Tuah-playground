            'https://i.imgur.com/kn5F6ps.png', // Sarbanes
            'https://i.imgur.com/IdvgtZQ.png', // Kohl
            'https://i.imgur.com/1gLFvuC.png', // Carper
            'https://i.imgur.com/M6eKLPM.png', // Stabenow
            'https://i.imgur.com/PukFyua.png', // Cantwell
            'https://i.imgur.com/k2LzbZg.png', // Dayton
            'https://i.imgur.com/BrBpvlH.png', // Corzine
            'https://i.imgur.com/6gaOorE.png', // Bill Nelson
            'https://i.imgur.com/CqqY3Uz.png', // Ben Nelson
            'https://i.imgur.com/7SgBfTQ.png'  // Conrad
        ];
    }

    // Add images to the right column
    imageUrls.forEach(url => {
        let img = document.createElement('img');
        img.src = url;
        img.style.width = '100%';
        img.style.height = 'auto';
        img.style.border = 'none';
        img.style.objectFit = 'contain';
        rightCol.appendChild(img);
    });

    // Add a second header for the next set of images
    secondHeader.style.color = 'white';
    secondHeader.style.textAlign = 'center';
    secondHeader.style.backgroundColor = 'rgba(23, 55, 107, 0.8)';
    secondHeader.style.padding = '8px';
    secondHeader.style.borderRadius = '5px';
    secondHeader.style.marginBottom = '10px'; // Add spacing below the header
    secondHeader.style.gridColumn = '1 / -1'; // Make the header span all columns
    secondHeader.classList.add('custom-h3');
    rightCol.appendChild(secondHeader);

    // Add the second set of images
    secondImageUrls.forEach(url => {
        let img = document.createElement('img');
        img.src = url;
        img.style.width = '100%';
        img.style.height = 'auto';
        img.style.border = 'none';
        img.style.objectFit = 'contain';
        rightCol.appendChild(img);
    });

    // Insert the Congressional Makeup screen into the DOM
    questions.parentNode.insertBefore(hqDiv, questions.nextSibling);
}

function handleMutations(mutationsList, observer) {
    if (observerRunning) return;
    observerRunning = true;

    addHeadquarterButton()

    observer.observe(document.documentElement, { childList: true, subtree: true });
    observerRunning = false;
}


let observerRunning = false;

var element = document.getElementById('controlElement');
if (!element) {
	let singleObserver = new MutationObserver(handleMutations);
	singleObserver.observe(document.documentElement, { childList: true, subtree: true });
	var controlElement = document.createElement('div');
	controlElement.style.display = 'none';
	controlElement.id = 'controlElement';
	document.body.appendChild(controlElement);
}
