/*
Welcome to Astro's Variable Headquarters
This is a fairly simple function you can add to your mods. It will create a little menu in which the player can view the numbers laid out within the functions.
This is a great way to give the player a little more information about your mod.

Across this code, you will find a lot of aspects you can edit. I have left plenty of comments to help you understand what each part does.
Don't be afraid to ask in #mod-help on the Discord for clarification or help with editing this code.

To use this, place this code below your Code 2, and edit it as needed. You can duplicate the 'let a = document.createElement('p');' section to add more lines of text.

Functions are referenced in text like so: ' + function + '. This will display the number from the function in the text.
Credit to Nina, who made the original code that summoned the new screen (originally for the 1960Red/1952Blue advisor screen)
Go ahead and use this code in your mods - just give me credit. I hope it helps you out!
*/

e=campaignTrail_temp

function addHeadquarterButton() {
    // Check if the button already exists
    if (document.getElementById('headquarter_button')) {
        return;
    }

    // Find the reference button by its ID
    const refButton = document.getElementById('view_electoral_map');
    if (!refButton) {
        return;
    }

    // Create a new button element
    const newButton = document.createElement('button');

    newButton.id = 'headquarter_button';

    newButton.style.marginLeft = '1.5em';

    newButton.innerHTML = 'Your Administration';

    // Attach the click event listener
    newButton.addEventListener('click', openHeadquarter);

    // Insert the new button next to the reference button
    refButton.insertAdjacentElement('afterend', newButton);
}

function getScoreMessage(label, value) { //dynamic text based on the value of the variable being tracked
    switch(label.toLowerCase()) {
        case 'wins':
            if (value > 8) return `You've conquered Olympus with ${value} conquests!`;
            else if (value > 3) return `You've collected the spoils from ${value} conquests.`;
            else return `You lament at having made ${value} conquests.`;
            
        case 'agenda':
            if (value > 5) return `Congress bows before you after having passed ${value} bills.`;
            else if (value > 1) return `Congress is weak but brave enough to stand in your way after having passed ${value} bills.`;
            else return `Congress smells weakness and is unwilling to yield an inch after having passed ${value} bills.`;
            
        case 'neolib':
            if (value > 5) return `You've embraced neoliberalism ${value} times.`;
            else if (value > 2) return `You've let neoliberal allies sway your decisions ${value} times.`;
            else return `Neoliberals feel left out of your administration with ${value} influence.`;
            
        case 'nato':
            if (value > 5) return `You have full command of NATO after impressing your allies ${value} times.`;
            else if (value > 2) return `NATO respects you after exercising leadership ${value} times.`;
            else return `NATO is reluctant to work with you after having made ${value} attempts to heal relations.`;
            
        case 'russia':
            if (value > 5) return `Cracks in the Earth are forming after you've torched relations with Putin ${value} times.`;
            else if (value > 2) return `Putin is furious with your reckless antics after having bent Russian relations ${value} times.`;
            else return `The Russian menace is calm but suspicious of you after you've rocked relations ${value} times.`;
            
        default:
            return `Your ${label} is ${value}`;
    }
}

function createScoreElement(label, value, parent) {
    const element = document.createElement('p');
    element.textContent = getScoreMessage(label, value);
    
    // Apply styling
    element.style.color = 'white';
    element.style.backgroundColor = 'rgba(23, 55, 107, 0.8)';
    element.style.padding = '8px';
    element.style.borderRadius = '5px';
    element.style.margin = '3px';
    
    parent.appendChild(element);
}

function openHeadquarter() {

    let questions = document.querySelector(".inner_window_question");

    questions.style.display = 'none';

    const hqButton = document.getElementById('headquarter_button');
    if (hqButton) {
        hqButton.remove();
    }

    // Create and set up the headquarters div
    let hqDiv = document.createElement('div');
    hqDiv.id = 'headquarter';
    hqDiv.style.display = 'flex'; // Set layout to flex to allow columns
    hqDiv.style.color = 'white';
    hqDiv.style.backgroundColor = '#383838';
    hqDiv.style.backgroundImage = 'url(https://i.imgur.com/F6xHPXG.jpeg)'; // Set background image. I would recommend using the same file size as the original image, ~959 x 327. Otherwise it will stretch the image. Comment or remove this line of text if you don't want a background image and instead want hqDiv.style.backgroundColor to set your background color
    hqDiv.style.backgroundSize = 'cover';
    hqDiv.style.border = '5px solid rgb(23, 55, 107)';
    hqDiv.style.borderRadius = '10px';
    hqDiv.style.height = '54%';
    hqDiv.style.flexDirection = 'column';
    hqDiv.style.alignItems = 'center';
    hqDiv.style.position = 'relative'; // Add this line

    // Create header div
    let headerDiv = document.createElement('div');
    headerDiv.style.display = 'flex';
    headerDiv.style.justifyContent = 'space-between';
    headerDiv.style.width = '100%';
    headerDiv.style.alignItems = 'center';

    // Add back button to the header
    let backButton = document.createElement('button');
    backButton.innerText = 'Back';
    backButton.style.margin = '5px';
    backButton.style.backgroundColor = 'grey';
    backButton.style.position = 'absolute';
    backButton.style.top = '16px';
    backButton.style.left = '10px';
    backButton.onclick = function() {
        // Hide headquarters and show the original content
        hqDiv.remove();
        questions.style.display = '';
    }
    headerDiv.appendChild(backButton);

    // This is a header text that you can add if you want to. It is not necessary. - Astro
    let headerText = document.createElement('h3');
    headerText.textContent = 'Your Administration';
    headerText.style.textAlign = 'center';  // Center the text
    headerText.style.flexGrow = '1';  // Make h3 take up the maximum space available
    headerText.style.backgroundColor = 'rgba(23, 55, 107, 0.8)'; // Set the background color to blue with some transparency
    headerText.style.padding = '8px'; // Add some padding for better appearance
    headerText.style.borderRadius = '5px'; // Optional: Adds rounded corners
    headerDiv.appendChild(headerText);
    

    // Append headerDiv to the main hqDiv
    hqDiv.appendChild(headerDiv);

    // Create a container div for both columns
    let columnsDiv = document.createElement('div');
    columnsDiv.style.display = 'flex';
    columnsDiv.style.width = '100%';

    // Create inner divs for columns
    let leftCol = document.createElement('div');
    leftCol.style.width = '50%';
    columnsDiv.appendChild(leftCol);

    let rightCol = document.createElement('div');
    rightCol.style.width = '50%';
    columnsDiv.appendChild(rightCol);

    // Append columnsDiv to the main hqDiv
    hqDiv.appendChild(columnsDiv);


    questions.parentNode.insertBefore(hqDiv, questions.nextSibling);

    // This is the bulk of the content you can edit. You can add more text or images here.

    // Create Bulk of the Text
    createScoreElement('wins', Win, leftCol);
    createScoreElement('agenda', Agenda, leftCol);
    createScoreElement('neolib', Neo, leftCol);
    createScoreElement('nato', Nato, leftCol);
    createScoreElement('russia', Russia, leftCol);

    // Style the right column for image-text pairs
    rightCol.style.display = 'flex';
    rightCol.style.flexDirection = 'column';
    rightCol.style.gap = '30px';  // Space between image-text pairs
    
    // First image-text pair (Speaker of the House)
    let pair1 = document.createElement('div');
    pair1.style.display = 'flex';
    pair1.style.alignItems = 'center';
    pair1.style.gap = '25px';
    pair1.style.justifyContent = 'flex-end';
    pair1.style.width = '100%';
    
    let img1 = document.createElement('img');
    // Default speaker image
    let speakerImage = 'https://i.imgur.com/ujzIE4A.png'; // Default Hastert image
    let speakerName = 'Dennis Hastert';
    
    // Update speaker based on variables
    if (typeof speakerDetermined !== 'undefined' && speakerDetermined) {
        if (Win > 7) {
            speakerImage = 'https://i.imgur.com/U8Jhc8t.png'; 
            speakerName = 'Nancy Pelosi<br><small>Clever girl</small>';
        } else if (Win > 5) {
            speakerImage = 'https://i.imgur.com/egn6waw.png'; 
            speakerName = 'John Boehner<br><small>Pain in the ass</small>';
        } else {
            speakerImage = 'https://i.imgur.com/JDDpRwY.png'; 
            speakerName = 'Roy Blunt<br><small>Obstructionist cuck</small>';
        }
    }
    
    img1.src = speakerImage;
    img1.style.width = '100px';
    img1.style.height = 'auto';
    img1.style.border = '3px solid black';
    img1.style.flexShrink = '0';
    
    let text1 = document.createElement('p');
    text1.innerHTML = `<small>Speaker of the House</small><br>${speakerName}`;
    text1.style.color = 'white';
    text1.style.backgroundColor = 'rgba(23, 55, 107, 0.8)';
    text1.style.padding = '8px';
    text1.style.borderRadius = '5px';
    text1.style.margin = '0';
    text1.style.textAlign = 'center';
    text1.style.flex = '1';
    
    pair1.appendChild(img1);
    pair1.appendChild(text1);

    // Second image-text pair (Foe tracker)
    let pair2 = document.createElement('div');
    pair2.style.display = 'flex';
    pair2.style.alignItems = 'center';
    pair2.style.gap = '25px';
    pair2.style.justifyContent = 'flex-end';
    pair2.style.width = '100%';
    
    let img2 = document.createElement('img');
    // Default foe image
    let foeImage = 'https://i.imgur.com/b0zFCwh.gif'; // Default image
    let foeName = 'Satan';
    let foeLabel = 'Likely Foe';
    
    // Update foe based on variables
    if (!foeDetermined) {
        if (Notroops > 0 && Loyalist > 0 && Gay > 0 && Dumb > 0) {
            foeImage = 'https://i.imgur.com/zZxfLYU.png'; 
            foeName = 'The Storm';
        } else if (Notroops > 0 && Loyalist > 0 && Tax > 0) {
            foeImage = 'https://i.imgur.com/eLhnMy7.png'; 
            foeName = 'The Hero';
        } else if (Troops > 0 && Patriot > 0 && Tax > 0) {
            foeImage = 'https://i.imgur.com/xpXhQCf.png'; 
            foeName = 'The Madman';
        } else if (Notroops > 0 && Loyalist > 0 && Gay > 0) {
            foeImage = 'https://i.imgur.com/xfCwi5O.png'; 
            foeName = 'The Hunter';
        } else if (Notroops > 0 && Loyalist > 0) {
            foeImage = 'https://i.imgur.com/x9sSwK9.png'; 
            foeName = 'The Maverick';
        } else if (Notroops > 0) {
            foeImage = 'https://i.imgur.com/MTwy7bN.png'; 
            foeName = 'The King';
        } else if (Troops > 0 && Patriot > 0 && Gay > 0) {
            foeImage = 'https://i.imgur.com/i5LyHYY.png'; 
            foeName = 'The God Warrior';
        } else if (Troops > 0) {
            foeImage = 'https://i.imgur.com/9gSLBJz.png'; 
            foeName = 'The Commoner';
        } else {
            foeImage = 'https://i.imgur.com/b0zFCwh.gif'; 
            foeName = 'Partisan Hack';
        }
    }

    if (foeDetermined) {
        if (Dem == 8) {
            foeImage = 'https://i.imgur.com/mYlZH3q.png'; 
            foeName = 'Dick Cheney<br><small>Olympia Snowe</small>';
        } else if (Dem == 7) {
            foeImage = 'https://i.imgur.com/DJ529PW.png'; 
            foeName = 'Sam Brownback<br><small>Michele Bachmann</small>';
        } else if (Dem == 6) {
            foeImage = 'https://i.imgur.com/HvDWV76.png'; 
            foeName = 'Mike Huckabee<br><small>Fred Thompson</small>';
        } else if (Dem == 5) {
            foeImage = 'https://i.imgur.com/rKYHvr1.png'; 
            foeName = 'Ron Paul<br><small>Gary Johnson</small>';
        } else if (Dem == 4) {
            foeImage = 'https://i.imgur.com/zrebC42.png'; 
            foeName = 'Rudy Giuliani<br><small>Mike Pence</small>';
        } else if (Dem == 3) {
            foeImage = 'https://i.imgur.com/tRIFmwd.png'; 
            foeName = 'Jeb Bush<br><small>Tom Ridge</small>';
        } else if (Dem == 2) {
            foeImage = 'https://i.imgur.com/ZQAeYjB.png'; 
            foeName = 'Mitt Romney<br><small>Charlie Crist</small>';
        } else if (Dem == 1) {
            foeImage = 'https://i.imgur.com/aSJJQp7.png'; 
            foeName = 'John McCain<br><small>Joe Lieberman</small>';
        } else {
            foeImage = 'https://i.imgur.com/b0zFCwh.gif'; 
            foeName = 'Partisan Hack';
        }
    }

    if (foeDetermined) {
        foeLabel = 'Your Foe';
    }
    
    img2.src = foeImage;
    img2.style.width = '100px';
    img2.style.height = 'auto';
    img2.style.border = '3px solid black';
    img2.style.flexShrink = '0';
    
    let text2 = document.createElement('p');
    text2.innerHTML = `<small>${foeLabel}</small><br>${foeName}`;
    text2.style.color = 'white';
    text2.style.backgroundColor = 'rgba(23, 55, 107, 0.8)';
    text2.style.padding = '8px';
    text2.style.borderRadius = '5px';
    text2.style.margin = '0';
    text2.style.textAlign = 'center';
    text2.style.flex = '1';
    
    pair2.appendChild(img2);
    pair2.appendChild(text2);
    
    // Add the pairs to the right column
    rightCol.appendChild(pair1);
    rightCol.appendChild(pair2);
    
}

function handleMutations(mutationsList, observer) {
    if (observerRunning) return;
    observerRunning = true;

    addHeadquarterButton()

    observer.observe(document.documentElement, { childList: true, subtree: true });
    observerRunning = false;
}


let observerRunning = false;

var element = document.getElementById('controlElement');
if (!element) {
	let singleObserver = new MutationObserver(handleMutations);
	singleObserver.observe(document.documentElement, { childList: true, subtree: true });
	var controlElement = document.createElement('div');
	controlElement.style.display = 'none';
	controlElement.id = 'controlElement';
	document.body.appendChild(controlElement);
}
