/*
Welcome to Astro's Variable Headquarters
This is a fairly simple function you can add to your mods. It will create a little menu in which the player can view the numbers laid out within the functions.
This is a great way to give the player a little more information about your mod.

Across this code, you will find a lot of aspects you can edit. I have left plenty of comments to help you understand what each part does.
Don't be afraid to ask in #mod-help on the Discord for clarification or help with editing this code.

To use this, place this code below your Code 2, and edit it as needed. You can duplicate the 'let a = document.createElement('p');' section to add more lines of text.

Functions are referenced in text like so: ' + function + '. This will display the number from the function in the text.
Credit to Nina, who made the original code that summoned the new screen (originally for the 1960Red/1952Blue advisor screen)
Go ahead and use this code in your mods - just give me credit. I hope it helps you out!
*/

e=campaignTrail_temp

function addHeadquarterButton() {
    // Check if the button already exists
    if (document.getElementById('headquarter_button')) {
        return;
    }

    // Find the reference button by its ID
    const refButton = document.getElementById('view_electoral_map');
    if (!refButton) {
        return;
    }

    // Create a new button element
    let buttonlabel = 'Your Administration';
    
    if ([0, 7, 9, 14].includes(e.noCounter)) {
        buttonlabel = '<strong><span style="color: red;">!</span></strong> Your Administration';
    }
    
    const newButton = document.createElement('button');
    
    newButton.id = 'headquarter_button';
    newButton.style.marginLeft = '1.5em';
    
    // Use template literals to correctly interpolate the buttonlabel
    newButton.innerHTML = `${buttonlabel}`;
    
    // Attach the click event listener
    newButton.addEventListener('click', openHeadquarter);
    
    // Insert the new button next to the reference button
    refButton.insertAdjacentElement('afterend', newButton);
}

function getScoreMessage(label, value) { //dynamic text based on the value of the variable being tracked
    switch(label.toLowerCase()) {
        case 'wins':
            if (value > 8) return `You've conquered Olympus with ${value} conquests!`;
            else if (value > 3) return `You've collected the spoils from ${value} conquests.`;
            else return `You lament at having made ${value} conquests.`;
            
        case 'agenda':
            if (value > 5) return `Congress bows before you after having passed ${value} bills.`;
            else if (value > 1) return `Congress is weak but brave enough to stand in your way after having passed ${value} bills.`;
            else return `Congress smells weakness and is unwilling to yield an inch after having passed ${value} bills.`;
            
        case 'neolib':
            if (value > 5) return `You've embraced neoliberalism ${value} times.`;
            else if (value > 2) return `You've let neoliberal allies sway your decisions ${value} times.`;
            else return `Neoliberals feel left out of your administration with ${value} influence.`;
            
        case 'nato':
            if (value > 5) return `You have full command of NATO after impressing your allies ${value} times.`;
            else if (value > 2) return `NATO respects you after exercising leadership ${value} times.`;
            else return `NATO is reluctant to work with you after having made ${value} attempts to heal relations.`;
            
        case 'russia':
            if (value > 5) return `Cracks in the Earth are forming after you've torched relations with Putin ${value} times.`;
            else if (value > 2) return `Putin is furious with your reckless antics after having bent Russian relations ${value} times.`;
            else return `The Russian menace is calm but suspicious of you after you've rocked relations ${value} times.`;
            
        default:
            return `Your ${label} is ${value}`;
    }
}

function createScoreElement(label, value, parent) {
    const element = document.createElement('p');
    element.textContent = getScoreMessage(label, value);
    
    // Apply styling
    element.style.color = 'white';
    element.style.backgroundColor = 'rgba(23, 55, 107, 0.8)';
    element.style.padding = '8px';
    element.style.borderRadius = '5px';
    element.style.margin = '3px';
    element.classList.add('custom-p');
    
    parent.appendChild(element);
}

function openHeadquarter() {
    let questions = document.querySelector(".inner_window_question");
    questions.style.display = 'none';

    const existingHqDiv = document.getElementById('headquarter');
    if (existingHqDiv) {
        existingHqDiv.remove();
    }

    const hqButton = document.getElementById('headquarter_button');
    if (hqButton) {
        hqButton.remove();
    }

    // Create and set up the headquarters div
    let hqDiv = document.createElement('div');
    hqDiv.id = 'headquarter';
    hqDiv.style.display = 'flex';
    hqDiv.style.color = 'white';
    hqDiv.style.backgroundColor = '#383838';
    hqDiv.style.backgroundImage = 'url(https://i.imgur.com/F6xHPXG.jpeg)';
    hqDiv.style.backgroundSize = 'cover';
    hqDiv.style.border = '5px solid rgb(23, 55, 107)';
    hqDiv.style.borderRadius = '10px';
    hqDiv.style.height = '54%';
    hqDiv.style.flexDirection = 'column';
    hqDiv.style.alignItems = 'center';
    hqDiv.style.position = 'relative';

    // Create header div
    let headerDiv = document.createElement('div');
    headerDiv.style.display = 'flex';
    headerDiv.style.justifyContent = 'space-between';
    headerDiv.style.width = '100%';
    headerDiv.style.alignItems = 'center';

    // Add back button to the header
    let backButton = document.createElement('button');
    backButton.innerText = 'Back';
    backButton.style.margin = '5px';
    backButton.style.backgroundColor = 'grey';
    backButton.style.position = 'absolute';
    backButton.style.top = '16px';
    backButton.style.left = '10px';
    backButton.onclick = function() {
        hqDiv.remove();
        questions.style.display = '';
    }
    headerDiv.appendChild(backButton);

    // Add a button to switch to the Congressional Makeup page
    let congressButton = document.createElement('button');
    congressButton.innerText = 'Battle for Olympus';
    congressButton.style.margin = '5px';
    congressButton.style.backgroundColor = 'grey';
    congressButton.style.position = 'absolute';
    congressButton.style.top = '16px';
    congressButton.style.right = '10px';
    congressButton.onclick = function() {
        showCongressionalMakeup(hqDiv, questions);
    }
    headerDiv.appendChild(congressButton);

    // This is a header text that you can add if you want to. It is not necessary. - Astro
    let headerText = document.createElement('h3');
    headerText.textContent = 'Your Administration';
    headerText.style.textColor = 'white';
    headerText.style.textAlign = 'center';
    headerText.style.flexGrow = '1';
    headerText.style.backgroundColor = 'rgba(23, 55, 107, 0.8)';
    headerText.style.padding = '8px';
    headerText.style.borderRadius = '5px';
    headerText.classList.add('custom-h3');
    headerDiv.appendChild(headerText);

    // Append headerDiv to the main hqDiv
    hqDiv.appendChild(headerDiv);

    // Create a container div for both columns
    let columnsDiv = document.createElement('div');
    columnsDiv.style.display = 'flex';
    columnsDiv.style.width = '100%';

    // Create inner divs for columns
    let leftCol = document.createElement('div');
    leftCol.style.width = '50%';
    leftCol.classList.add('custom-p');
    columnsDiv.appendChild(leftCol);

    let rightCol = document.createElement('div');
    rightCol.style.width = '50%';
    columnsDiv.appendChild(rightCol);

    // Append columnsDiv to the main hqDiv
    hqDiv.appendChild(columnsDiv);

    questions.parentNode.insertBefore(hqDiv, questions.nextSibling);

    // Create Bulk of the Text
    createScoreElement('wins', Win, leftCol);
    createScoreElement('agenda', Agenda, leftCol);
    createScoreElement('neolib', Neo, leftCol);
    createScoreElement('nato', Nato, leftCol);
    createScoreElement('russia', Russia, leftCol);

    // Style the right column for image-text pairs
    rightCol.style.display = 'flex';
    rightCol.style.flexDirection = 'column';
    rightCol.style.gap = '30px';
    rightCol.classList.add('custom-p');

    // First image-text pair (Speaker of the House)
    let pair1 = document.createElement('div');
    pair1.style.display = 'flex';
    pair1.style.alignItems = 'center';
    pair1.style.gap = '25px';
    pair1.style.justifyContent = 'flex-end';
    pair1.style.width = '100%';

    let img1 = document.createElement('img');
    let speakerImage = 'https://i.imgur.com/ujzIE4A.png';
    let speakerName = 'Dennis Hastert';

    if (typeof speakerDetermined !== 'undefined' && speakerDetermined) {
        if (Win > 7) {
            speakerImage = 'https://i.imgur.com/U8Jhc8t.png';
            speakerName = 'Nancy Pelosi<br><small>Clever girl</small>';
        } else if (Win > 5) {
            speakerImage = 'https://i.imgur.com/egn6waw.png';
            speakerName = 'John Boehner<br><small>Pain in the ass</small>';
        } else {
            speakerImage = 'https://i.imgur.com/JDDpRwY.png';
            speakerName = 'Roy Blunt<br><small>Obstructionist cuck</small>';
        }
    }

    img1.src = speakerImage;
    img1.style.width = '100px';
    img1.style.height = 'auto';
    img1.style.border = '3px solid black';
    img1.style.flexShrink = '0';

    let text1 = document.createElement('p');
    text1.innerHTML = `<small>Speaker of the House</small><br>${speakerName}`;
    text1.style.color = 'white';
    text1.style.backgroundColor = 'rgba(23, 55, 107, 0.8)';
    text1.style.padding = '8px';
    text1.style.borderRadius = '5px';
    text1.style.margin = '0';
    text1.style.textAlign = 'center';
    text1.style.flex = '1';
    text1.classList.add('custom-p');

    pair1.appendChild(img1);
    pair1.appendChild(text1);

    // Second image-text pair (Foe tracker)
    let pair2 = document.createElement('div');
    pair2.style.display = 'flex';
    pair2.style.alignItems = 'center';
    pair2.style.gap = '25px';
    pair2.style.justifyContent = 'flex-end';
    pair2.style.width = '100%';

    let img2 = document.createElement('img');
    let foeImage = 'https://i.imgur.com/b0zFCwh.gif';
    let foeName = 'Satan';
    let foeLabel = 'Likely Foe';

    if (!foeDetermined) {
        if (Notroops > 0 && Loyalist > 0 && Gay > 0 && Dumb > 0) {
            foeImage = 'https://i.imgur.com/zZxfLYU.png';
            foeName = 'The Storm';
        } else if (Notroops > 0 && Loyalist > 0 && Tax > 0) {
            foeImage = 'https://i.imgur.com/eLhnMy7.png';
            foeName = 'The Hero';
        } else if (Troops > 0 && Patriot > 0 && Tax > 0) {
            foeImage = 'https://i.imgur.com/xpXhQCf.png';
            foeName = 'The Madman';
        } else if (Notroops > 0 && Loyalist > 0 && Gay > 0) {
            foeImage = 'https://i.imgur.com/xfCwi5O.png';
            foeName = 'The Hunter';
        } else if (Notroops > 0 && Loyalist > 0) {
            foeImage = 'https://i.imgur.com/x9sSwK9.png';
            foeName = 'The Maverick';
        } else if (Notroops > 0) {
            foeImage = 'https://i.imgur.com/MTwy7bN.png';
            foeName = 'The King';
        } else if (Troops > 0 && Patriot > 0 && Gay > 0) {
            foeImage = 'https://i.imgur.com/i5LyHYY.png';
            foeName = 'The God Warrior';
        } else if (Troops > 0) {
            foeImage = 'https://i.imgur.com/9gSLBJz.png';
            foeName = 'The Commoner';
        } else {
            foeImage = 'https://i.imgur.com/b0zFCwh.gif';
            foeName = 'Partisan Hack';
        }
    }

    if (foeDetermined) {
        if (Dem == 8) {
            foeImage = 'https://i.imgur.com/mYlZH3q.png';
            foeName = 'Dick Cheney<br><small>Olympia Snowe</small>';
        } else if (Dem == 7) {
            foeImage = 'https://i.imgur.com/DJ529PW.png';
            foeName = 'Sam Brownback<br><small>Michele Bachmann</small>';
        } else if (Dem == 6) {
            foeImage = 'https://i.imgur.com/HvDWV76.png';
            foeName = 'Mike Huckabee<br><small>Fred Thompson</small>';
        } else if (Dem == 5) {
            foeImage = 'https://i.imgur.com/rKYHvr1.png';
            foeName = 'Ron Paul<br><small>Gary Johnson</small>';
        } else if (Dem == 4) {
            foeImage = 'https://i.imgur.com/zrebC42.png';
            foeName = 'Rudy Giuliani<br><small>Mike Pence</small>';
        } else if (Dem == 3) {
            foeImage = 'https://i.imgur.com/tRIFmwd.png';
            foeName = 'Jeb Bush<br><small>Tom Ridge</small>';
        } else if (Dem == 2) {
            foeImage = 'https://i.imgur.com/ZQAeYjB.png';
            foeName = 'Mitt Romney<br><small>Charlie Crist</small>';
        } else if (Dem == 1) {
            foeImage = 'https://i.imgur.com/aSJJQp7.png';
            foeName = 'John McCain<br><small>Joe Lieberman</small>';
        } else {
            foeImage = 'https://i.imgur.com/b0zFCwh.gif';
            foeName = 'Partisan Hack';
        }
    }

    if (Dem == 8 && Econ == 2 && Nato > 5 && Heart == 1) {
        foeImage = 'https://i.imgur.com/asbpjdE.png';
        foeName = 'Olympia Snowe<br><small>John Ashcroft</small>';
    }

    if (foeDetermined) {
        foeLabel = 'Your Foe';
    }

    img2.src = foeImage;
    img2.style.width = '100px';
    img2.style.height = 'auto';
    img2.style.border = '3px solid black';
    img2.style.flexShrink = '0';

    let text2 = document.createElement('p');
    text2.innerHTML = `<small>${foeLabel}</small><br>${foeName}`;
    text2.style.color = 'white';
    text2.style.backgroundColor = 'rgba(23, 55, 107, 0.8)';
    text2.style.padding = '8px';
    text2.style.borderRadius = '5px';
    text2.style.margin = '0';
    text2.style.textAlign = 'center';
    text2.style.flex = '1';
    text2.classList.add('custom-p');

    pair2.appendChild(img2);
    pair2.appendChild(text2);

    // Add the pairs to the right column
    rightCol.appendChild(pair1);
    rightCol.appendChild(pair2);
}

function openWebBrowser() {
    let questions = document.querySelector(".inner_window_question");
    questions.style.display = 'none';

    // Clear the Congressional Makeup screen if it exists
    const existingCongressDiv = document.getElementById('headquarter');
    if (existingCongressDiv) {
        existingCongressDiv.remove();
    }

    // Create and set up the Web Browser div
    let webBrowserDiv = document.createElement('div');
    webBrowserDiv.id = 'web_browser';
    webBrowserDiv.style.display = 'flex';
    webBrowserDiv.style.flexDirection = 'column';
    webBrowserDiv.style.alignItems = 'center';
    webBrowserDiv.style.color = 'white';
    webBrowserDiv.style.backgroundColor = '#383838';
    webBrowserDiv.style.backgroundImage = 'url(https://i.imgur.com/CB8ErnK.png)';
    webBrowserDiv.style.backgroundSize = 'cover';
    webBrowserDiv.style.border = '5px solid rgb(23, 55, 107)';
    webBrowserDiv.style.borderRadius = '10px';
    webBrowserDiv.style.height = '85%';
    webBrowserDiv.style.position = 'relative';

    // Append the image container to the webBrowserDiv
    webBrowserDiv.appendChild(imageContainer);

    // Create header div
    let headerDiv = document.createElement('div');
    headerDiv.style.display = 'flex';
    headerDiv.style.justifyContent = 'space-between';
    headerDiv.style.width = '100%';
    headerDiv.style.alignItems = 'center';

    // Add a button to switch to the Congressional Makeup screen
    let congressButton = document.createElement('button');
    congressButton.innerText = 'Battle for Olympus';
    congressButton.style.margin = '5px';
    congressButton.style.backgroundColor = 'grey';
    congressButton.style.position = 'absolute';
    congressButton.style.top = '-5px';
    congressButton.style.left = '0px';
    congressButton.onclick = function () {
        webBrowserDiv.remove(); // Remove the Web Browser screen
        showCongressionalMakeup(); // Recreate the Congressional Makeup screen
    };
    headerDiv.appendChild(congressButton);

    // Append headerDiv to the main webBrowserDiv
    webBrowserDiv.appendChild(headerDiv);

    // Create a container for the bulleted list
    let bulletListContainer = document.createElement('div');
    bulletListContainer.id = 'bullet-list-container';
    bulletListContainer.style.position = 'absolute'; // Use absolute positioning
    bulletListContainer.style.top = '270px'; // Move it 50px from the top
    bulletListContainer.style.left = '325px'; // Move it 20px from the left
    bulletListContainer.style.width = '280px';
    bulletListContainer.style.color = 'black';
    bulletListContainer.style.textAlign = 'left';
    bulletListContainer.style.fontSize = '16px';
    bulletListContainer.style.overflowY = 'auto'; // Add a vertical scrollbar
    bulletListContainer.style.maxHeight = '240px';

    // Create an unordered list element
    let bulletList = document.createElement('ul');
    bulletList.id = 'decision-list';
    bulletList.style.listStyleType = 'disc';
    bulletList.style.paddingLeft = '20px';

    // Append the list to the container
    bulletListContainer.appendChild(bulletList);

    // Append the bullet list container to the webBrowserDiv
    webBrowserDiv.appendChild(bulletListContainer);

    // Function to add a decision to the bulleted list
    function addDecisionToBulletList(decisionText) {
        let listItem = document.createElement('li');
        listItem.innerText = decisionText;
        bulletList.appendChild(listItem);
    }

    const decisionMessages = {
        5148: 'You have passed the Clean Air Plan.',
        5150: 'You have held a NATO summit.',
        5149: 'You have increased funding for the Citizen Corps and American Corps.',
        20890: 'You have axed the Bush Tax Cuts.',
        22666: 'You have achieved minor reforms to No Child Left Behind.',
        21279: 'You have achieved minor reforms to No Child Left Behind.',
        22670: 'You have majorly reformed No Child Left Behind.',
        5155: 'You have repealed the PATRIOT Act.',
        22460: 'You have passed the Cyber Intelligence Protection Act.',
        5263: 'You have set an Iraqi withdrawal date.',
        5262: 'You have proposed a plan to rebuild Iraq.',
        20943: 'You have lowered prescription costs for seniors.',
        24708: 'You have passed ClarkCare.',
        22483: 'You have passed ClarkCare.',
        24734: 'You have passed ClarkCare.',
        24700: "You have repealed Don't Ask, Don't Tell.",
        22475: "You have repealed Don't Ask, Don't Tell.",
        24713: "You have repealed Don't Ask, Don't Tell.",
        24726: "You have repealed Don't Ask, Don't Tell.",
        24704: 'You have renewed the assault weapons ban.',
        22479: 'You have renewed the assault weapons ban.',
        24730: 'You have renewed the assault weapons ban.',
        22758: 'You have negotiated the JCPOA nuclear agreement.',
        22490: 'You have passed stimulus for families and banks.',
        23790: 'You have passed stimulus for families and banks.',
        24739: 'You have passed stimulus for families and banks.',
        20969: 'You have passed minor stimulus.',
        24748: 'You have passed the Financial Stimulus and Corporate Reform Act.',
        24635: 'You have passed Universal Preschool.',
        22874: 'You have saved Fannie Mae and Freddie Mac.',
        20964: 'You have saved Fannie Mae and Freddie Mac.',
        23796: 'You have saved Fannie Mae and Freddie Mac.',
        24745: 'You have saved Fannie Mae and Freddie Mac.',
        23781: 'You have created the Homeland and Hometown Security Fund.',
        23772: 'You have passed a union card check bill.',
        22493: 'You have passed bank bailouts.',
        23793: 'You have passed bank bailouts.',
        24742: 'You have passed bank bailouts.',
        22761: 'You have passed U.N. resolutions to sanction Iran.',
        22035: 'You have deposed Iraqi Prime Minister al-Maliki.',
        20946: 'You have visited the troops in the Middle East.',
        23483: 'You have deployed a coalition of nations to stabilize Iraq.',
        5246: 'You have created the Troubled Asset Relief Program.',
        22053: 'You have created the Troubled Asset Relief Program.',
        22555: 'You have created the Troubled Asset Relief Program.',
        23718: 'You have created the Troubled Asset Relief Program.',
        23733: 'You have created the Troubled Asset Relief Program.',
        22507: 'You have created the Troubled Asset Relief Program.',
        22511: 'You have created the Troubled Asset Relief Program and the Foreclosure Relief Program.',
        22057: 'You have created the Troubled Asset Relief Program and the Foreclosure Relief Program.',
        22559: 'You have created the Troubled Asset Relief Program and the Foreclosure Relief Program.',
        23722: 'You have created the Troubled Asset Relief Program and the Foreclosure Relief Program.',
        23737: 'You have created the Troubled Asset Relief Program and the Foreclosure Relief Program.',
        22514: 'You have passed the Clinton Plan.',
        22562: 'You have passed the Clinton Plan.',
        22565: 'You have passed tax hikes on the top 1%, reestablished a combination of a Resolution Trust Corporation and a Reconstruction Finance Corporation, and reestablished the Home Owners’ Loan Corporation.',
        23728: 'You have passed tax hikes on the top 1%, reestablished a combination of a Resolution Trust Corporation and a Reconstruction Finance Corporation, and reestablished the Home Owners’ Loan Corporation.',
        21552: 'You have provided military aid to Georgia.',
        24183: 'You have killed Osama bin Laden.',
        24162: 'You have convinced NATO to save Georgia.',
    };
    
    e.player_answers.forEach(answer => {
        if (decisionMessages[answer]) {
            addDecisionToBulletList(decisionMessages[answer]);
        }
    });

    // Insert the Web Browser screen into the DOM
    questions.parentNode.insertBefore(webBrowserDiv, questions.nextSibling);
}

function showCongressionalMakeup() {
    let questions = document.querySelector(".inner_window_question");
    questions.style.display = 'none';

    // Hide other parts of the screen
    const candidatePic = document.getElementById('candidate_pic');
    const runningMatePic = document.getElementById('running_mate_pic');
    const signDisplay = document.querySelector('.inner_window_sign_display');

    if (candidatePic) candidatePic.style.display = 'none';
    if (runningMatePic) runningMatePic.style.display = 'none';
    if (signDisplay) signDisplay.style.display = 'none';

    // Clear any existing Congressional Makeup screen
    const existingCongressDiv = document.getElementById('headquarter');
    if (existingCongressDiv) {
        existingCongressDiv.remove();
    }

    // Create and set up the Congressional Makeup div
    let hqDiv = document.createElement('div');
    hqDiv.id = 'headquarter';
    hqDiv.style.display = 'flex';
    hqDiv.style.flexDirection = 'column';
    hqDiv.style.alignItems = 'center';
    hqDiv.style.color = 'white';
    hqDiv.style.backgroundColor = '#383838';
    hqDiv.style.backgroundImage = 'url(https://i.imgur.com/tut7DC0.jpeg)';
    hqDiv.style.backgroundSize = 'cover'; 
    hqDiv.style.backgroundRepeat = 'no-repeat';
    hqDiv.style.border = '5px solid rgb(23, 55, 107)';
    hqDiv.style.borderRadius = '10px';
    hqDiv.style.height = '85%';
    hqDiv.style.position = 'relative';
    hqDiv.style.overflow = 'hidden';

    // Create header div
    let headerDiv = document.createElement('div');
    headerDiv.style.display = 'flex';
    headerDiv.style.justifyContent = 'space-between';
    headerDiv.style.width = '100%';
    headerDiv.style.alignItems = 'center';

    // Add a button to switch back to the Administration page
    let adminButton = document.createElement('button');
    adminButton.innerText = 'Your Administration';
    adminButton.style.margin = '5px';
    adminButton.style.backgroundColor = 'grey';
    adminButton.style.position = 'absolute';
    adminButton.style.top = '16px';
    adminButton.style.left = '10px';
    adminButton.onclick = function () {
        hqDiv.remove();
        // Restore hidden elements when switching back to Administration page
        if (candidatePic) candidatePic.style.display = 'block';
        if (runningMatePic) runningMatePic.style.display = 'block';
        if (signDisplay) signDisplay.style.display = 'block';
        openHeadquarter();
    };
    headerDiv.appendChild(adminButton);

    // Add a button to switch to the Web Browser page
    let browserButton = document.createElement('button');
    browserButton.innerText = 'whitehouse.gov';
    browserButton.style.margin = '5px';
    browserButton.style.backgroundColor = 'grey';
    browserButton.style.position = 'absolute';
    browserButton.style.top = '16px';
    browserButton.style.right = '10px';
    browserButton.onclick = function () {
        hqDiv.remove();
        openWebBrowser();
    };
    headerDiv.appendChild(browserButton);

    // Add header text
    let headerText = document.createElement('h3');
    headerText.textContent = 'Battle for Olympus';
    headerText.style.color = 'white';
    headerText.style.textAlign = 'center';
    headerText.style.flexGrow = '1';
    headerText.style.backgroundColor = 'rgba(23, 55, 107, 0.8)';
    headerText.style.padding = '8px';
    headerText.style.borderRadius = '5px';
    headerText.classList.add('custom-h3');
    headerDiv.appendChild(headerText);

    // Append headerDiv to the main hqDiv
    hqDiv.appendChild(headerDiv);

    // Create a container div for both columns
    let columnsDiv = document.createElement('div');
    columnsDiv.style.display = 'flex';
    columnsDiv.style.width = '100%';
    columnsDiv.style.height = '100%';

    // Create inner divs for columns
    let leftCol = document.createElement('div');
    leftCol.style.width = '50%';
    leftCol.style.textAlign = 'center';
    leftCol.style.overflowY = 'auto'; // Add vertical scrollbar if needed
    leftCol.style.height = '100%'; // Take full height of the parent
    leftCol.style.padding = '10px'; // Add padding for better spacing
    columnsDiv.appendChild(leftCol);

    let rightCol = document.createElement('div');
    rightCol.style.width = '75%'; // Take full width of the parent
    rightCol.style.height = '100%'; // Take full height of the parent
    rightCol.style.overflowY = 'auto'; // Enable vertical scrolling
    rightCol.style.overflowX = 'hidden'; // Disable horizontal scrolling
    rightCol.style.display = 'grid';
    rightCol.style.gridTemplateColumns = 'repeat(2, 1fr)'; // 2 columns
    rightCol.style.gridTemplateRows = 'repeat(4, 1fr)'; // 4 rows
    rightCol.style.gap = '10px'; // Add gap between grid items
    rightCol.style.padding = '10px'; // Add padding for better spacing
    rightCol.style.paddingBottom = '60px'; // Add buffer at the bottom
    rightCol.style.boxSizing = 'border-box';
    columnsDiv.appendChild(rightCol);

    // Append columnsDiv to the main hqDiv
    hqDiv.appendChild(columnsDiv);

    // Add Senate section
    let senateSection = document.createElement('div');
    senateSection.style.marginBottom = '20px';
    senateSection.style.marginTop = '0'; // Set to 0 to remove any default margin
    senateSection.style.paddingTop = '0';
    senateSection.style.position = 'relative';
    
    // Add Senate image
    let senateImg = document.createElement('img');
    let senateImage = 'https://i.imgur.com/qqLWi7A.png'; // Default image
    
    if (speakerDetermined == true) {
        console.log("Speaker is determined. Checking Win conditions...");
        console.log("Win value:", Win);
        console.log("Schiavo value:", Schiavo);
        
        if (Win > 9) {
            console.log("Win over 9");
            senateImage = 'https://i.imgur.com/aqMjWQI.png'; 
        }
        else if (Win > 8 && Win <= 9) {
            console.log("Win over 8");
            senateImage = 'https://i.imgur.com/Q7JMn7F.png'; 
        }
        else if (Win > 7 && Win <= 8) {
            console.log("Win over 7");
            senateImage = 'https://i.imgur.com/vOOfSGt.png'; 
        }
        else if (Win > 6 && Win <= 7 && e.player_answers.includes(20752)) { // Not helping Lamont
            console.log("Win over 6, no help to Lamont");
            senateImage = 'https://i.imgur.com/C53V6mW.png';
        }
        else if (Win > 6 && Win <= 7 && (e.player_answers.includes(5184) || e.player_answers.includes(20755))) { // Helping Lamont
            console.log("Win over 6, help Lamont");
            senateImage = 'https://i.imgur.com/7rDgCSE.png';
        }
        else if (Win > 5 && Win <= 6 && e.player_answers.includes(20752)) { // Not helping Lamont
            console.log("Win over 5, no Lamont");
            senateImage = 'https://i.imgur.com/PwpZLM0.png';
        }
        else if (Win > 5 && Win <= 6 && (e.player_answers.includes(5184) || e.player_answers.includes(20755))) { // Helping Lamont
            console.log("Win over 5, Lamont");
            senateImage = 'https://i.imgur.com/eT7WMa6.png';
        }
        else if (Win > 4 && Win <= 5 && Schiavo == 1) {
            console.log("Win over 4, Schiavo");
            senateImage = 'https://i.imgur.com/Seyd90G.png';
        }
        else if (Win > 4 && Win <= 5 && Schiavo == 0) {
            console.log("Win over 4, no Schiavo");
            senateImage = 'https://i.imgur.com/di3LO4k.png';
        }
        else if (Win > 3 && Win <= 4 && Schiavo == 1) {
            console.log("Win over 3, Schiavo");
            senateImage = 'https://i.imgur.com/o3eQEkG.png';
        }
        else if (Win > 3 && Win <= 4 && Schiavo == 0) {
            console.log("Win over 3, no Schiavo");
            senateImage = 'https://i.imgur.com/L5Q5k1R.png';
        }
        else if (Win > 2 && Win <= 3 && Schiavo == 1) {
            console.log("Win over 2, Schiavo");
            senateImage = 'https://i.imgur.com/2b9KFgd.png';
        }
        else if (Win > 2 && Win <= 3 && Schiavo == 0) {
            console.log("Win over 2, no Schiavo");
            senateImage = 'https://i.imgur.com/XojZ27U.png'; 
        }
        else if (Win > 1 && Win <= 2 && Schiavo == 1) {
            console.log("Win over 1, Schiavo");
            senateImage = 'https://i.imgur.com/IQ9OpeD.png';
        }
        else if (Win > 1 && Win <= 2 && Schiavo == 0) {
            console.log("Win over 1, no Schiavo");
            senateImage = 'https://i.imgur.com/iBvIfuM.png'; 
        }
        else if (Win > 0 && Win <= 1) {
            console.log("Win over 0")
            senateImage = 'https://i.imgur.com/3Z8gtpY.png';
        }
        else if (Win <= 0) {
            senateImage = 'https://i.imgur.com/v0xHy3r.png';
        }
    }
    
    // Set the image source and styles
    senateImg.src = senateImage;
    senateImg.style.width = '95%';
    senateImg.style.height = 'auto';
    senateImg.style.border = '';
    senateImg.style.position = 'absolute';
    senateImg.style.top = '80px';
    senateImg.style.left = '10px';
    
    // Add the image to the Senate section
    senateSection.appendChild(senateImg);
    leftCol.appendChild(senateSection);

    // Add a header above the array of images
    let challengersHeader = document.createElement('h3');
    challengersHeader.textContent = 'Challengers';
    challengersHeader.style.color = 'white';
    challengersHeader.style.textAlign = 'center';
    challengersHeader.style.backgroundColor = 'rgba(23, 55, 107, 0.8)';
    challengersHeader.style.padding = '8px';
    challengersHeader.style.borderRadius = '5px';
    challengersHeader.style.marginBottom = '10px'; // Add spacing below the header
    challengersHeader.style.gridColumn = '1 / -1'; // Make the header span all columns
    challengersHeader.classList.add('custom-h3');
    rightCol.appendChild(challengersHeader);

    let secondHeader = document.createElement('h3');
