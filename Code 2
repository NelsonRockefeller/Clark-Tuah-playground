            $("#game_window")[0].style.borderColor = "#f6f6f4";
            $("#game_window")[0].style.backgroundColor = "#f6f6f4";
            campaignTrail_temp.candidate_image_url = 'https://i.imgur.com/JV85kqq.png';
            campaignTrail_temp.running_mate_image_url = 'https://i.imgur.com/XZC6hgY.png';
            corrr=`\n<h2>NEW AMERICAN PATRIOTISM</h2>`;
