cyoAdventure = function(a) {
    // Initialize variables
    ans = campaignTrail_temp.player_answers[campaignTrail_temp.player_answers.length - 1];
    let pop_vote = e.current_results[0];
    let playerPolling = (pop_vote.find(p => p.pk === e.candidate_id)).pvp;
    let i = 0;

    // Sort pop_vote by pk
    pop_vote.sort((a, b) => a.pk - b.pk);
    e.noCounter += 1;

    ///Promise tax cut repeal or get gay
    if(ans == 5139 ){
        Promise += 1;
    }
    if(ans == 5140 ){
        Promise += 2;
    }
    if(ans == 20731 ){
        Promise += 3;
    }
    if(ans == 22843 ){
        Promise += 4;
    }

    //Wins to determine midterms
    if (ans == 5148 || ans == 5150 || ans == 5143 || ans == 20752 || ans == 5155 || ans == 20890 || ans == 23844 || ans == 22625 || ans == 23197 || ans == 24794) {
        Win += 1;
    }
    if (ans == 20740 || ans == 22670 || ans == 20755 || ans == 23832 || ans == 21037) {
        Win += 2;
    }
    if (ans == 22940) {
        Win += 3;
    }
    if (ans == 23192 || ans == 22622 || ans == 22632) {
        Win -= 1;
    }
    if (ans == 23202) {
        Win -= 2;
    }

    // Domestic Agenda passed tracker
    if (ans == 5148 || ans == 22670 || ans == 22460 || ans == 20890 || ans == 22475 || ans == 22479 || ans == 22483 || ans == 22490 || ans == 23781 || ans == 23772 || ans == 23790 || ans == 5155 || ans == 5149 || ans == 20934 || ans == 20943 || ans == 24635 || ans == 24700 || ans == 24704 || ans == 24713 || ans == 24726 || ans == 24730 || ans == 24734 || ans == 24794) {
        Agenda += 1;
    }

    //Foreign policy successes
    if (ans == 25448 || ans == 25444 || ans == 25435 || ans == 25431 || ans == 22853 || ans == 20915 || ans == 20924 || ans == 5190 || ans == 5147 || ans == 22758 || ans == 22043 || ans == 5150 || ans == 20877 || ans == 24059 || ans == 24062 || ans == 23695 || ans == 20946 || ans == 24170 || ans == 24768 || ans == 24772 || ans == 24764 || ans == 24776 || ans == 24780 || ans == 24785 || ans == 24797 || ans == 24800) {
        Nato += 1;
    }
    if (ans == 23483 || ans == 22035 || ans == 22761 || ans == 24794 || ans == 25439) {
        Nato += 2;
    }

    //neoliberling
    if (ans == 20946 ||ans == 24797 || ans == 22853 || ans == 20773 || ans == 20902 || ans == 22456 || ans == 22460 || ans == 22765 || ans == 24052 || ans == 24056 || ans == 24170 || ans == 5149 || ans == 22035 || ans == 22039 || ans == 24035 || ans == 24039) {
        Neo += 1;
    } 
    if (ans == 22768 || ans == 20747 || ans == 5262 || ans == 22761) {
        Neo += 2;
    }

    //rusher? i hardly even know her
    if (ans == 22847 || ans == 21552 || ans == 21561 || ans == 21566 || ans == 5237  || ans == 5235  || ans == 21471 || ans == 24150 || ans == 24158 || ans == 22580) {
        Russia += 2;
    } 
    if (ans == 5236  || ans == 24154 || ans == 21536 || ans == 22573 || ans == 20777 || ans == 22863 || ans == 24790) {
        Russia += 1;
    } 
    if (ans == 22584) {
        Russia += 3;
    } 
    if (ans == 24162) {
        Russia += 10;
    }

    //Boner is your speaker
    if (ans == 20899 || ans == 20906 || ans == 20902) {
        Boner += 1;
    }

    //girlboss Pelosi is your speaker
    if (ans == 22453 || ans == 22456 || ans == 22460) {
        Pelosi += 1;
    }
    
    //Dont ask dont tell gets krilled
    if (ans == 22475) {
        Dadt += 1;
    }

    //try to kill doma
    if(ans == 20897 ){
        Gay += 1;
    }

    //kill Bushs tax cuts
    if(ans == 20890 ){
        Tax += 1;
    }

    //fail to pass your gay pet project
    if(ans == 20777 || ans == 20906 ){
        Dumb += 1;
    }

    ///reveal the memo
    if(ans == 20740 ){
        Schiavo += 1;
    }

    //keep patriot act
    if(ans == 20747 ){
        Patriot += 1;
    }

    //kill patriot act
    if(ans == 5155 ){
        Loyalist += 1;
    }

    ///Troop surge
    if(ans == 5262 ){
        Troops += 1;
    }

    //iraq withdrawal
    if(ans == 5263 ){
        Notroops += 1;
    }

    //investigate blackwater
    if(ans == 24059 || ans == 24062 ){
        Blackwater += 1;
    }

    //huntin bin Laden
    if(ans == 23695 || ans == 24170 ){
        Hunt += 1;
    }

    // Specific NATO wins tracker
    if (ans == 5150 || ans == 20924) {
        Britain += 1;
    }
    if (ans == 5147) {
        Germany += 1;
        Turkey += 1;  
    }
    if (ans == 20877 || ans == 5263 || ans == 20928) {
        France += 1;
    }

    ///pass your pet project that no one cares about
    if(ans == 22460 ){
        Cyber += 1;
    }

    //do you save financial institutions
    if (ans == 20964 || ans == 20969 || ans == 22490 || ans == 22493 || ans == 22874 || ans == 23790 || ans == 23793 || ans == 23796 ||ans == 24739 || ans == 24742 || ans == 24745 || ans == 24748) {
        Fred += 1;
    }

    // Fall out with killdawg means no HOLC for you loserrr
    if (ans == 23701 || ans == 23705 || ans == 23709) {
        Hillary += 1; 
    }

    //how bigly do you save the economy
    if (ans == 5246 || ans == 22053 || ans == 22507 || ans == 22555 || ans == 23718 || ans == 23733) {
        Econ += 1;
    } 
    if (ans == 22057 || ans == 22511 || ans == 22559 || ans == 23722 || ans == 23737) {
        Econ += 2;
    }
    if (ans == 22514 || ans == 22562) {
        Econ += 3;
    }
    if (ans == 22565 || ans == 23728) {
        Econ += 4;
    }

    //ww3 baybee
    if(ans == 24162 || ans == 22610 ){
        War += 1;
    }

    //gay mccain allegations
    if (ans == 21244 || ans == 21338 || ans == 22728) {
        Buch += 1;
    }

    //2000 election conspiracytok
    if (ans == 21501 || ans == 22639 || ans == 22642) {
        Crazy += 1;
    }
    if (ans == 22636 || ans == 22639 || ans == 22642) {
        Crazy += 2;
    }

    //Rudy coup
    if (ans == 25457 || ans == 254213 || ans == 254243) {
        Messiah += 1;
    }

    //dicks heart go boom
    if(ans == 24331 ){
        Heart += 1;
    }

    //we got him
    if(ans == 24183 ){
        Kill += 1;
    }

    //shitty way of making admin tab work
    if (e.noCounter == 7) {
        speakerDetermined = true;
    }

    //another shitty way to work the admin tab
    if (e.noCounter == 14) {
        foeDetermined = true;
    }

    // Opponent banner trigger
    if (e.noCounter == 22) {
        Opp += 1;
    }

    ///my schizo way of getting clark banner back
    if(e.noCounter == 23) {
        Back += 1;
    }

    /// Promise tax cut repeal or get gay
    if (e.noCounter == 3) {   
        if (Promise == 1 && Win > 1) {
            campaignTrail_temp.questions_json[3] = tunnel(22665); 
        }
        if (Promise == 2) {
            campaignTrail_temp.questions_json[3] = tunnel(20896); 
        }
        if (Promise == 3 && Win > 1) {
            campaignTrail_temp.questions_json[3] = tunnel(20889); 
        }
        else if (Promise == 3) {
            campaignTrail_temp.questions_json[3] = tunnel(22673); 
        }
        if (Promise == 4 && Nato > 0) {
            campaignTrail_temp.questions_json[3] = tunnel(24789); 
        }
        else if (Promise == 4) {
            campaignTrail_temp.questions_json[3] = tunnel(22846); 
        }
    }

    /// Kill patriot act or nah
    if (e.noCounter == 5) {
        if (ans == 5155 && Gay == 1) {
            campaignTrail_temp.questions_json[5] = tunnel(22621); 
        }
        else if (ans == 5155) {
            campaignTrail_temp.questions_json[5] = tunnel(20888); 
        }

        if (ans == 20747 && Gay == 1) {
            campaignTrail_temp.questions_json[5] = tunnel(22621); 
        }
        else if (ans == 20747 && Tax == 1) {
            campaignTrail_temp.questions_json[5] = tunnel(22698); 
        }
        else if (ans == 20747) {
            campaignTrail_temp.questions_json[5] = tunnel(23831); 
        }
    }

    /// Troop surge X No surge
    if (e.noCounter == 9) {
        if (Troops == 1 && Pelosi == 0) {
            campaignTrail_temp.questions_json[9] = tunnel(24051); 
        }
        else if (Troops == 1) {
            campaignTrail_temp.questions_json[9] = tunnel(20923); 
        }

        if (Notroops == 1) {
            campaignTrail_temp.questions_json[9] = tunnel(20909); 
        }
    }

    /// Foreign Policy - Isolationist
    if (ans == 6666) {
        campaignTrail_temp.questions_json[13] = tunnel(21008);
        campaignTrail_temp.questions_json[18] = tunnel(21145); 
    }

    /// Hastert drake allegations
    if (e.noCounter == 6 && Win > 5) {
        campaignTrail_temp.questions_json[6] = tunnel(22939); 
    }

    /// wins determining who yo speaker is
    if (e.noCounter == 7) {
        if (Win > 5) {
            campaignTrail_temp.questions_json[7] = tunnel(20898); 
        }
        if (Win > 7) {
            campaignTrail_temp.questions_json[7] = tunnel(22452); 
        }
    }

    /// midterm based bills
    if (e.noCounter == 10) {
        if (Gay > 0 && Win < 6) {
            campaignTrail_temp.questions_json[10] = tunnel(22681); 
        }
        if (Win > 5 || Win < 8) {
            campaignTrail_temp.questions_json[10] = tunnel(20934); 
        }
        if (Win > 9) {
            campaignTrail_temp.questions_json[10] = tunnel(24725); 
        }
        else if (Win > 8 && Notroops == 1) {
            campaignTrail_temp.questions_json[10] = tunnel(22474); 
        }
        else if (Win > 8 && Troops == 1) {
            campaignTrail_temp.questions_json[10] = tunnel(24699); 
        }
        else if (Win < 8 && Win > 7) {
            campaignTrail_temp.questions_json[10] = tunnel(24712); 
        }
    }

    /// Foreign policy event or domestic clark tuah
    if (e.noCounter == 11) {
        if (Troops > 0) {
            campaignTrail_temp.questions_json[11] = tunnel(22757); 
        }
        if (Notroops > 0 && Hunt > 0) {
            campaignTrail_temp.questions_json[11] = tunnel(24166); 
        }
        else if (Notroops > 0 && Agenda > 4 && Win > 9) {
            campaignTrail_temp.questions_json[11] = tunnel(23771); 
        }
        else if (Notroops > 0 && Agenda > 4) {
            campaignTrail_temp.questions_json[11] = tunnel(24738); 
        }
    }

    /// Economy exploded
    if (e.noCounter == 12) {
        if (Boner > 0) {
            campaignTrail_temp.questions_json[12] = tunnel(20957); 
        }
        if (Pelosi > 0 && Agenda > 5 && Win > 9) {
            campaignTrail_temp.questions_json[12] = tunnel(24738); 
        }
        else if (Pelosi > 0 && Agenda > 5) {
            campaignTrail_temp.questions_json[12] = tunnel(23789); 
        }
        else if (Pelosi > 0) {
            campaignTrail_temp.questions_json[12] = tunnel(22489); 
        }
    }

    /// DNC based on troop surge or nah and or freddie mac collapse
    if (e.noCounter == 13) {
        if (Troops > 0 && Fred > 0) {
            campaignTrail_temp.questions_json[13] = tunnel(20974);
                if (Agenda < 4) {
                    swapAnswers(campaignTrail_temp.answers_json, 20974, 20975, 209752, 9999); }
                if (Neo < 6 ) {
                    swapAnswers(campaignTrail_temp.answers_json, 20974, 20976, 209762, 9999); }
                if (Nato < 6 ) {
                    swapAnswers(campaignTrail_temp.answers_json, 20974, 20977, 209772, 9999); }
                if (Pelosi == 0 || Fred == 0) {
                    swapAnswers(campaignTrail_temp.answers_json, 20974, 20978, 209782, 9999); }
        } else if (Troops > 0) {
            campaignTrail_temp.questions_json[13] = tunnel(25345);
                if (Agenda < 4) {
                    swapAnswers(campaignTrail_temp.answers_json, 25345, 25346, 253462, 9999); }
                if (Neo < 6 ) {
                    swapAnswers(campaignTrail_temp.answers_json, 25345, 25350, 253502, 9999); }
                if (Nato < 6 ) {
                    swapAnswers(campaignTrail_temp.answers_json, 25345, 25355, 253552, 9999); }
                if (Pelosi == 0 || Fred == 0) {
                    swapAnswers(campaignTrail_temp.answers_json, 25345, 25360, 253602, 9999); }
        } else if (Notroops > 0 && Fred > 0) {
            campaignTrail_temp.questions_json[13] = tunnel(21008);
                if (Agenda < 4 ) {
                    swapAnswers(campaignTrail_temp.answers_json, 21008, 21009, 210092, 9999); }
                if (Nato < 6 ) {
                    swapAnswers(campaignTrail_temp.answers_json, 21008, 21012, 210122, 9999); }
                if (Pelosi == 0 || Fred == 0) {
                    swapAnswers(campaignTrail_temp.answers_json, 21008, 21020, 210202, 9999); }
        } else if (Notroops > 0) {
            campaignTrail_temp.questions_json[13] = tunnel(25394);
                if (Agenda < 4 ) {
                    swapAnswers(campaignTrail_temp.answers_json, 25394, 25395, 253952, 9999); }
                if (Nato < 6 ) {
                    swapAnswers(campaignTrail_temp.answers_json, 25394, 25399, 253992, 9999); }
                if (Pelosi == 0 || Fred == 0) {
                    swapAnswers(campaignTrail_temp.answers_json, 25394, 25408, 254082, 9999); }
        }
    }

    /// RNC
